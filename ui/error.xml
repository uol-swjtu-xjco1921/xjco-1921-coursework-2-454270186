<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkWindow" id="invalid_window">
    <property name="title">ERROR</property>
    <property name="can_focus">False</property>
    <property name="default_width">300</property>
    <property name="default_height">200</property>
    <child>
      <object class="GtkBox" id="box_main">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="label_message">
            <property name="visible">True</property>
            <property name="label">Invalid input</property>
            <property name="height-request">70</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="back_button">
            <property name="label">Back</property>
            <property name="visible">True</property>
            <property name="height-request">130</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>